 //Start--Java program to illustrate whether the number 11 is prime or not
    static boolean checkForPrime (int n){
    int count = 0; 
    int i = 1;
    while(i <= n ) {
        if(n % i == 0){
            count = count + 1;
        } 
        i = i + 1 ;
    }
    if(count == 2) 
        return true;
    else
      return false;
    
    } 
}
     System.out.println(checkForPrime(int n));